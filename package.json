{
    "name": "rn-generator",
    "displayName": "RN-Generator",
    "description": "React Native Generator",
    "version": "0.3.0",
    "publisher": "NhanCDT",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "activationEvents": [
        "onCommand:extension.generateRN"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateRN",
                "title": "Generate RN"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./src/snippets/navsnippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
